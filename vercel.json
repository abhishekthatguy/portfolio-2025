{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/submit-form.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/submit-form",
      "dest": "api/submit-form.py"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.9"
  }
}

